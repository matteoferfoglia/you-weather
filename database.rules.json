{
  // This rules are for the emulators, copy them in the real Firebase Realtime Database
  "rules": {

    // Only authenticated users can access the database
    ".read": "auth.uid != null",
    ".write": "auth.uid != null",

    "it_units_youweather_entities_storage_WeatherReport": { // use index to improve performance for queries "orderByChild"
      ".indexOn": [
        "reporterUserId" // name of field on which queries apply
      ]

    }
  }
}